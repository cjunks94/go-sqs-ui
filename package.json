{
  "name": "go-sqs-ui",
  "version": "1.0.0",
  "description": "AWS SQS UI application with Go backend and modern JavaScript frontend",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint --max-warnings 0 'static/**/*.js' 'test/**/*.js'",
    "lint:fix": "eslint --fix 'static/**/*.js' 'test/**/*.js'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "happy-dom": "^12.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^1.0.0"
  }
}
